[NetLink] 
NAME = NetLink 
DEFAULT_ACTIVE = 1 
CONNECT_LOGICS = Repeater Logic: 34, NetLogic: 35

[NetLogic] 
TYPE=Repeater 
RX=NetRx 
TX=NetTx 
CALLSIGN=NONE 
EVENT_HANDLER=/usr/share/svxlink/events.tcl 
DEFAULT_LANG=en_US
OPEN_ON_SQL=50 
OPEN_SQL_FLANK=OPEN 
NO_REPEAT=1 
IDLE_TIMEOUT=10

#Needed for RemoterTRX & Remote SvxLinks
[NetRx]
TYPE=Net
HOST=remote.rx.host
TCP_PORT=5210
#LOG_DISCONNECTS_ONCE=0
AUTH_KEY="Change this key now!"
CODEC=S16
