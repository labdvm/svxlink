[LinkLogic]
TYPE=Simplex
RX=LinkRx1
TX=LinkTx1
MODULES=ModuleHelp,ModuleRemoteRelay
CALLSIGN=MYCALL
#MACROS=Macros
SHORT_IDENT_INTERVAL=20
LONG_IDENT_INTERVAL=60
#IDENT_ONLY_AFTER_TX=4
# ACTIVATE_MODULE_ON_LONG_CMD=2:EchoLink
EVENT_HANDLER=/usr/share/svxlink/events.tcl
DEFAULT_LANG=en_US
EXEC_CMD_ON_SQL_CLOSE=500
RGR_SOUND_DELAY=0
REPORT_CTCSS=71.9
#TX_CTCSS=SQL_OPEN
FX_GAIN_NORMAL=0
FX_GAIN_LOW=-12
ONLINE_CMD=112233
MUTE_RX_ON_TX=1

[LinkRx1]
TYPE=Local
AUDIO_DEV=alsa:plughw:0
AUDIO_CHANNEL=0
#tyde de detecvion VOX,CTCSS,SERIAL,EVDEV,SIGLEV,GPIO
# SQL_DET=CTCSS
GPIO_SQL_PIN=gpio18
# d√©but du squelch
SQL_START_DELAY=0
# retard de detection 20 -100ms
SQL_DELAY=20
# fin de detection 
SQL_HANGTIME=20
#SQL_EXTENDED_HANGTIME=1000
#SQL_EXTENDED_HANGTIME_THRESH=1000
SQL_TIMEOUT=300
VOX_FILTER_DEPTH=60
VOX_THRESH=1200
CTCSS_FQ=71.9
DEEMPHASIS=0
SQL_TAIL_ELIM=20
PREAMP=0
PEAK_METER=0
DTMF_DEC_TYPE=INTERNAL
DTMF_MUTING=1
DTMF_HANGTIME=100
1750_MUTING=1

[LinkTx1]
TYPE=Local
AUDIO_DEV=alsa:plughw:1
AUDIO_CHANNEL=0
PTT_PORT=GPIO
PTT_TYPE=GPIO
PTT_PIN=gpio17
TIMEOUT=300
TX_DELAY=10
#CTCSS_FQ=71.9
#CTCSS_LEVEL=9
PREEMPHASIS=0
DTMF_TONE_LENGTH=100
DTMF_TONE_SPACING=50
DTMF_DIGIT_PWR=-18